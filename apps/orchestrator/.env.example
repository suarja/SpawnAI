# Orchestrator Configuration
PORT=3001
NODE_ENV=development

# API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock

# Security
JWT_SECRET=your_jwt_secret_here

# VM Configuration
VM_TIMEOUT_HOURS=24
VM_MAX_CPU=2
VM_MAX_MEMORY=4g
VM_MAX_STORAGE=10g

# Logging
LOG_LEVEL=info

# Development
DEBUG=spawnai:*